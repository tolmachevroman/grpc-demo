{
  "name": "grpc-dashboard-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean-schema": "rm -rf src/generated && mkdir -p src/generated",
    "generate-schema": "npm run clean-schema && protoc --plugin=./node_modules/.bin/protoc-gen-ts --ts_out=./src/generated --ts_opt=generate_dependencies,long_type_string --proto_path=../proto ../proto/*.proto"
  },
  "dependencies": {
    "@protobuf-ts/grpcweb-transport": "^2.11.1",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@types/google-protobuf": "^3.15.12",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "google-protobuf": "^4.0.0",
    "grpc-web": "^2.0.1",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.11.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "protoc": "^32.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3",
    "vite": "7.1.5"
  }
}
